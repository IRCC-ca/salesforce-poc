{
  "name": "ds-accessibility-demo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "npm run write:env -s && ng build",
    "deploy-build": "ng build",
    "start": "npm run write:env -s && ng serve --proxy-config proxy.conf.js",
    "serve:sw": "npm run build -s && npx http-server ./dist -p 4200",
    "lint": "ng lint && stylelint \"src/**/*.scss\"",
    "test": "npm run write:env -s && ng test",
    "test:ci": "npm run write:env -s && npm run lint -s && ng run ds-accessibility-demo:test:ci",
    "write:env": "ngx-scripts env npm_package_version",
    "generate": "ng generate",
    "translations:toJson": "node ./src/translations/ngx-translation-extract.js -r -i src/translations/DS-AccessibilityDemo-Strings.txt -l en-US,fr-FR -o src/translations",
    "translations:toText": "node src/translations/ngx-translation-extract.js -l en-US,fr-FR -i src/translations/ -o src/translations/DS-AccessibilityDemo-Strings.txt --clean ",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/en-US.json ./src/translations/fr-FR.json --clean --sort --format namespaced-json --marker _"
  },
  "dependencies": {
    "@angular/animations": "~14.2.5",
    "@angular/common": "~14.2.5",
    "@angular/compiler": "~14.2.5",
    "@angular/core": "~14.2.5",
    "@angular/forms": "~14.2.5",
    "@angular/localize": "~14.2.5",
    "@angular/platform-browser": "~14.2.5",
    "@angular/platform-browser-dynamic": "~14.2.5",
    "@angular/router": "~14.2.5",
    "@angular/service-worker": "~14.2.5",
    "@auth0/angular-jwt": "^5.1.2",
    "@fortawesome/fontawesome-free": "^6.1.2",
    "@ircc-ca/ds-sdc-core": "^1.5.6-beta.0",
    "@ngx-translate/core": "^14.0.0",
    "@popperjs/core": "^2.11.0",
    "amplify": "^0.0.11",
    "aws-amplify": "^5.0.9",
    "ircc-ds-angular-component-library": "^1.1.6-beta.0",
    "rxjs": "^7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4",
    "fast-csv": "~4.3.6",
    "dot-object": "~2.1.4"
  },
  "devDependencies": {
    "@angular-builders/jest": "^14.0.1",
    "@angular-devkit/build-angular": "~14.2.5",
    "@angular-eslint/builder": "~14.0.3",
    "@angular-eslint/eslint-plugin": "~14.0.3",
    "@angular-eslint/eslint-plugin-template": "~14.0.3",
    "@angular-eslint/schematics": "~14.0.3",
    "@angular-eslint/template-parser": "~14.0.3",
    "@angular/cli": "~14.2.5",
    "@angular/compiler-cli": "~14.2.5",
    "@angular/language-service": "~14.2.5",
    "@biesbjerg/ngx-translate-extract": "^7.0.3",
    "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
    "@ngneat/until-destroy": "^9.0.0",
    "@ngx-rocket/scripts": "^5.2.2",
    "@types/jest": "^28.1.8",
    "@types/node": "^14.18.26",
    "@typescript-eslint/eslint-plugin": "~5.34.0",
    "@typescript-eslint/parser": "~5.34.0",
    "eslint": "^8.3.0",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsdoc": "latest",
    "eslint-plugin-prefer-arrow": "latest",
    "hads": "^3.0.0",
    "https-proxy-agent": "^5.0.0",
    "jest": "^28.1.3",
    "postcss": "^8.4.5",
    "stylelint": "~14.11.0",
    "stylelint-config-recommended-scss": "~7.0.0",
    "stylelint-config-standard": "~28.0.0",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.1.0",
    "typescript": "~4.7.4"
  }
}
